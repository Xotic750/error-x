    "node": "*"
  },
  "license": "MIT",
  "dependencies": {
    "circular-json": "^0.3.1",
    "define-properties-x": "^1.3.0",
    "error-stack-parser": "^2.0.0",
    "find-index-x": "^1.1.0",
    "has-to-string-tag-x": "^1.2.0",
    "inspect-x": "^1.2.0",
    "is-callable": "^1.1.3",
    "is-error-x": "^1.2.0",
    "is-nil-x": "^1.2.0",
    "is-object-like-x": "^1.2.0",
    "is-primitive": "^2.0.0",
    "is-regex": "^1.0.4",
    "lodash._arraymap": "^3.0.0",
    "lodash.isnull": "^3.0.0",
    "safe-to-string-x": "^1.3.0",
    "stackframe": "^1.0.2",
    "string.prototype.trim": "^1.1.2",
    "to-length-x": "^1.2.0",
    "validate.io-undefined": "^1.0.3"
  },
  "devDependencies": {
    "@ljharb/eslint-config": "^11.0.0",
    "browserify": "^14.1.0",
    "browserify-derequire": "^0.9.4",
    "es5-shim": "^4.5.9",
    "es6-shim": "^0.35.3",
    "es7-shim": "^6.0.0",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "jasmine-node": "^1.14.5",
    "jsdoc-to-markdown": "^3.0.0",
    "make-jasmine-spec-runner-html": "^1.1.0",
    "nodemon": "^1.11.0",
    "nsp": "^2.6.3",
    "replace-x": "^1.1.1",
    "uglify-js": "^2.8.20"
  },
  "scripts": {
    "build": "npm run build:setver && npm run security && npm run eslint && npm run browserify && npm run uglify && npm run docs && npm run build:jasmine && npm test",
    "build:jasmine": "make-jasmine-spec-runner-html",
    "build:setver": "PKG_VER=$(node -p -e \"require('./package.json').version\") && replace-x \" @version .*\" \" @version ${PKG_VER}\" index.js",
    "build:watch": "nodemon --watch index.js --watch package.json --exec 'npm run build'",
    "docs": "jsdoc2md --name-format --example-lang js index.js > README.md",
    "eslint": "eslint *.js tests/spec/*.js",
    "security": "nsp check",
    "test": "jasmine-node --matchall tests/spec/",
    "test:watch": "nodemon --watch tests/spec/test.js --exec 'npm test'",
    "browserify": "browserify -p browserify-derequire -e index.js -o lib/error-x.js -u 'crypto' -s returnExports",
    "uglify": "uglifyjs lib/error-x.js -o lib/error-x.min.js --support-ie8 --compress --keep-fnames --mangle --beautify ascii_only=true,beautify=false --source-map lib/error-x.map"
  }
}
