  "bugs": {
    "url": "https://github.com/Xotic750/error-x/issues"
  },
  "engines": {
    "node": "*"
  },
  "license": "MIT",
  "dependencies": {
    "circular-json": "^0.3.1",
    "define-properties-x": "^1.3.0",
    "error-stack-parser": "^2.0.1",
    "find-index-x": "^1.1.0",
    "has-to-string-tag-x": "^1.2.0",
    "inspect-x": "^1.3.0",
    "is-callable": "^1.1.3",
    "is-error-x": "^1.2.0",
    "is-nil-x": "^1.2.0",
    "is-object-like-x": "^1.2.0",
    "is-primitive": "^2.0.0",
    "is-regex": "^1.0.4",
    "lodash._arraymap": "^3.0.0",
    "lodash.isnull": "^3.0.0",
    "safe-to-string-x": "^1.3.0",
    "stackframe": "^1.0.3",
    "string.prototype.trim": "^1.1.2",
    "to-length-x": "^1.2.0",
    "truncate-x": "^1.0.0",
    "validate.io-undefined": "^1.0.3"
  },
  "devDependencies": {
    "@xotic750/eslint-config-standard-x": "^1.0.3",
    "browserify": "^14.4.0",
    "browserify-derequire": "^0.9.4",
    "cross-env": "^5.0.1",
    "es5-shim": "^4.5.9",
    "es6-shim": "^0.35.3",
    "es7-shim": "^6.0.0",
    "eslint": "^3.19.0",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-css-modules": "^2.7.1",
    "eslint-plugin-eslint-comments": "^1.0.1",
    "eslint-plugin-jsdoc": "^3.1.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-no-use-extend-native": "^0.3.12",
    "husky": "^0.13.4",
    "jasmine-node": "^1.14.5",
    "jsdoc-to-markdown": "^3.0.0",
    "json3": "^3.3.2",
    "make-jasmine-spec-runner-html": "^1.1.0",
    "nodemon": "^1.11.0",
    "nsp": "^2.6.3",
    "parallelshell": "^3.0.1",
    "replace-x": "^1.1.1",
    "rimraf": "^2.6.1",
    "serve": "^5.2.2",
    "uglify-js": "^3.0.18"
  },
  "scripts": {
    "clean": "rimraf README.md lib/*",
    "clean:jasmine": "rimraf tests/index.html tests/run.js",
    "clean:all": "npm run clean:jasmine &&  npm run clean",
    "build": "npm run clean && npm run lint && npm run browserify && npm run uglify && npm run docs && npm test",
    "build:jasmine": "npm run clean:jasmine && make-jasmine-spec-runner-html",
    "build:setver": "PKG_VER=$(node -p -e \"require('./package.json').version\") && replace-x \" @version .*\" \" @version ${PKG_VER}\" index.js",
    "build:all": "npm run clean && npm run build:setver && npm run security && npm run lint && npm run browserify && npm run uglify && npm run docs && npm run build:jasmine && npm test",
    "start": "parallelshell \"serve\" \"nodemon --watch index.js --exec 'npm run build'\"",
    "docs": "jsdoc2md --name-format --example-lang js index.js > README.md",
    "lint": "eslint *.js tests/spec/*.js",
    "security": "nsp check",
    "test": "jasmine-node --matchall tests/spec/",
    "browserify": "browserify -p browserify-derequire -e index.js -o lib/module-boilerplate-x.js -u 'crypto' -s returnExports",
    "uglify": "uglifyjs lib/module-boilerplate-x.js -o lib/module-boilerplate-x.min.js --support-ie8 --compress --keep-fnames --mangle --beautify ascii_only=true,beautify=false --source-map filename=lib/module-boilerplate-x.map",
    "precommit": "npm run build:all",
    "prepush": "npm run build:all"
  }
}
