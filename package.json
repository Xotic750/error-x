    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "source-map-loader": "^0.2.4",
    "strip-ansi": "^5.2.0",
    "terser-webpack-plugin": "^1.4.1",
    "typescript": "^3.5.3",
    "webpack": "^4.39.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.7",
